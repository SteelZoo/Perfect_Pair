package com.olduo.last_dance.model.dao;

import com.olduo.last_dance.model.dto.User;

public interface UserDao {
	
	/**
     * 사용자 정보를 등한다.
     * @param user
     * @return
     */
    int insert(User user);
    
    
    /**
     * 사용자 정보를 조회한다.
     * @param userId
     * @return
     */
    User select(String user_id);
    
    
    /**
     * 사용자 정보를 수정한다.
     * @param user
     * @return
     */
    int update(User user);
    
    
    /**
     * 사용자 정보를 삭제한다.
     * @param userId
     * @return
     */
    int delete(String user_id);
    
}
